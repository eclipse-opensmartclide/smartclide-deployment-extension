{
  "name": "eclipse-che-deployment",
  "version": "0.0.1",
  "license": "MIT",
  "keywords": [
    "theia-extension"
  ],
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@theia/core": "1.18.0-next.120",
    "@theia/output": "1.18.0-next.120",
    "@types/node": "16.11.9",
    "recharts": "2.1.6"
  },
  "devDependencies": {},
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "build": "tsc -b",
    "watch": "tsc -w"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/widget-frontend-module",
      "backend": "lib/node/backend-module"
    }
  ]
}